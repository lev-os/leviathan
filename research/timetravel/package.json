{
  "name": "timetravel-research",
  "version": "1.0.0",
  "description": "Production-quality AI research platform with CLI and web interface",
  "main": "dist/api/server.js",
  "bin": {
    "timetravel": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && npm run build:web",
    "build:web": "cd src/web && npm run build",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\" \"npm run dev:cli\"",
    "dev:api": "tsx watch src/api/server.ts",
    "dev:web": "cd src/web && npm run dev",
    "dev:cli": "tsx watch src/cli/index.ts",
    "start": "node dist/api/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src",
    "docs": "typedoc src --out docs/api",
    "install:web": "cd src/web && npm install"
  },
  "keywords": ["research", "ai", "cli", "web", "analysis"],
  "author": "TimeTravel Research Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "fs-extra": "^11.2.0",
    "yaml": "^2.3.4",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
    "ws": "^8.14.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/ws": "^8.5.10",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "concurrently": "^8.2.2",
    "typedoc": "^0.25.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}