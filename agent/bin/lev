#!/bin/bash
# Leviathan Hexagonal Architecture CLI
# Routes through new CLI adapter with core SDK and extensible plugin support

# Set up environment
LEV_AGENT_PATH="$HOME/digital/leviathan/agent"
CONTEXTS_PATH="$LEV_AGENT_PATH/contexts"

# Route through new CLI adapter
CONTEXTS_PATH="$CONTEXTS_PATH" node "$LEV_AGENT_PATH/src/adapters/cli/cli-entry.js" "$@"