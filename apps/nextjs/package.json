    "@lev-os/validators": "workspace:*",
    "@t3-oss/env-nextjs": "^0.13.4",
    "@tanstack/react-query": "^5.62.7",
    "@trpc/client": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@trpc/tanstack-react-query": "^11.0.0",
    "geist": "^1.3.1",
    "next": "^15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "superjson": "2.2.2",
    "zod": "^3.24.1"
