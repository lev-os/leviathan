{
  "name": "llm-config-dashboard",
  "version": "1.0.0",
  "description": "Real-time dashboard for LLM-First Configuration System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"node server.js\" \"npm run client\"",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "cors": "^2.8.5",
    "axios": "^1.6.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "llm",
    "ai",
    "configuration",
    "automation",
    "dashboard"
  ],
  "author": "Kingly Research Lab",
  "license": "MIT"
}